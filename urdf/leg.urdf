<?xml version="1.0"?>
<robot name="leg">

  <!-- "world" link -->
  <link name="world"/>
  <gazebo reference="world">
    <static>true</static>
  </gazebo>

  <!--////////////////////////////////   link    ////////////////////////////////-->

  <link name="base">
    <collision>
      <origin xyz="0 0 0.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 1"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0.5" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 1"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0.5" rpy="0 0 0"/>
      <mass value="10"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
    </inertial>
  </link>
  <gazebo reference="base">
    <material>Gazebo/Orange</material>
  </gazebo>


  <link name="actuator1">
    <collision>
      <origin xyz="0 0.0381 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="0.0762" radius="0.073"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0.0381 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="0.0762" radius="0.073"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0.0381 0" rpy="1.5708 0 0"/>
      <mass value="1.75"/>
      <inertia ixx="0.001541527" ixy="0.000007051" ixz="-0.000001356" iyy="0.001938779" iyz="0.000000659" izz="0.001547151"/>
    </inertial>
  </link>
  <gazebo reference="actuator1">
    <material>Gazebo/Grey</material>
  </gazebo>


  <link name="actuator2">
    <collision>
      <origin xyz="0 0.073 0" rpy="0 1.5708 0"/>
      <geometry>
        <cylinder length="0.0762" radius="0.073"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0.073 0" rpy="0 1.5708 0"/>
      <geometry>
        <cylinder length="0.0762" radius="0.073"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0.073 0" rpy="0 1.5708 0"/>
      <mass value="1.75"/>
      <inertia ixx="0.001541527" ixy="0.000007051" ixz="-0.000001356" iyy="0.001938779" iyz="0.000000659" izz="0.001547151"/>
    </inertial>
  </link>
  <gazebo reference="actuator2">
    <material>Gazebo/Grey</material>
  </gazebo>


  <link name="actuator3">
    <collision>
      <origin xyz="0.0381 0 0" rpy="0 1.5708 0"/>
      <geometry>
        <cylinder length="0.0762" radius="0.073"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0.0381 0 0" rpy="0 1.5708 0"/>
      <geometry>
        <cylinder length="0.0762" radius="0.073"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0.0381 0 0" rpy="0 1.5708 0"/>
      <mass value="1.75"/>
      <inertia ixx="0.001541527" ixy="0.000007051" ixz="-0.000001356" iyy="0.001938779" iyz="0.000000659" izz="0.001547151"/>
    </inertial>
  </link>
  <gazebo reference="actuator3">
    <material>Gazebo/Grey</material>
  </gazebo>


  <link name="link1">
    <collision>
      <origin xyz="0.04 0 -0.125" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.08 0.25"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0.04 0 -0.125" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.08 0.25"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0.04 0 -0.125" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.00574167" ixy="0" ixz="0" iyy="0.00574167" iyz="0" izz="0.00106667"/>
    </inertial>
  </link>
  <gazebo reference="link1">
    <material>Gazebo/Yellow</material>
  </gazebo>


  <link name="link2">
    <collision>
      <origin xyz="0.04 0 -0.125" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.08 0.25"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0.04 0 -0.125" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.08 0.25"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0.04 0 -0.125" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.00574167" ixy="0" ixz="0" iyy="0.00574167" iyz="0" izz="0.00106667"/>
    </inertial>
  </link>
  <gazebo reference="link2">
    <material>Gazebo/White</material>
  </gazebo>



  <!--////////////////////////////////   joint    ////////////////////////////////-->

  <joint name="fixed_base" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="base"/>
  </joint>

  <joint name="joint1" type="continuous">
    <origin xyz="0 0.05 0.8" rpy="0 0 0"/>
    <parent link="base"/>
    <child link="actuator1"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="1.5"/>
  </joint>

  <joint name="fixed_actuator_1" type="fixed">
    <origin xyz="0 0.0762 0" rpy="0 0 0"/>
    <parent link="actuator1"/>
    <child link="actuator2"/>
    <!--
    <axis xyz="1 0 0"/>
    <limit effort="1000" upper="0" lower="0" velocity="0"/>
    -->
  </joint>

  <joint name="fixed_actuator_2" type="fixed">
    <origin xyz="0.0381 0.073 0" rpy="0 0 0"/>
    <parent link="actuator2"/>
    <child link="actuator3"/>
  </joint>
  
  <joint name="joint2" type="continuous">
    <origin xyz="0.0762 0 0" rpy="0 0 0"/>
    <parent link="actuator3"/>
    <child link="link1"/>
    <axis xyz="1 0 0"/>
    <dynamics damping="1.5"/>
  </joint>

  <joint name="joint3" type="continuous">
    <origin xyz="0 0 -0.25" rpy="0 0 0"/>
    <parent link="link1"/>
    <child link="link2"/>
    <axis xyz="1 0 0"/>
    <dynamics damping="1.5"/>
  </joint>


  <!--////////////////////////////////   plugin    ////////////////////////////////-->

  <gazebo>
    <plugin name="leg_plugin" filename="libleg_plugin.so">
    </plugin>
  </gazebo>

</robot>
